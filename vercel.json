{
  "version": 2,
  "rewrites": [
    { "source": "/service-worker.js", "destination": "/api/index.js" },
    { "source": "/manifest.json", "destination": "/api/index.js" },
    { "source": "/s/:id", "destination": "/api/index.js" },
    { "source": "/cdn/(.*)", "destination": "/api/index.js" },
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/health", "destination": "/api/index.js" }
  ]
}
